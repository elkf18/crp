    "build:debug": "cd android && ./gradlew assembleDebug --warning-mode all",
    "build:apk": "cd android && ./gradlew assembleRelease --warning-mode all",
    "publish:prod": "appcenter codepush release-react -a kelava/jokopi -d Production",
