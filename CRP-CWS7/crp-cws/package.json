    "ios": "react-native run-ios",
    "web": "expo start --web",
    "start": "react-native start",
    "build:ios": "react-native run-ios --configuration=release",
    "clean:ios": "rm -rf ios/Pods && rm -rf ios/build && cd ios && pod install && cd ../",
    "clean:project": "rm -rf node_modules && rm yarn.lock && yarn install",
    "upgrade": "react-native upgrade --legacy true",
    "eject": "react-native eject",
    "help": "react-native --help",
